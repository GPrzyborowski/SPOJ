#include <iostream>
#include <math.h>

using namespace std;

int main() {

	int N;
	string S;
	int odlegloscOdSrodka;

	cin >> N;
	cin >> S;

	N = S.length();

	if (N % 2 != 0 && N >= 1 && N <= 29) {
		char* kropki = new char[N];

		for (int i = 0; i < N; i++) {
			kropki[i] = '.';
		}

		for (int i = N / 2; i >= 0; i--) {
			odlegloscOdSrodka = abs(N / 2 - i);
			kropki[N / 2 - odlegloscOdSrodka] = S[N / 2 - odlegloscOdSrodka];
			kropki[N / 2 + odlegloscOdSrodka] = S[N / 2 + odlegloscOdSrodka];
			for (int j = 0; j < N; j++) {
				cout << kropki[j];
			}
			cout << endl;
		}
	}
	else {
		return 0;
	}
}
